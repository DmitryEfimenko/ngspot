{
  "name": "ng-superclass-demo",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/ng-superclass/demo/src",
  "prefix": "ngs",
  "targets": {
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "tsConfig": "packages/ng-superclass/demo/tsconfig.spec.json",
        "karmaConfig": "packages/ng-superclass/demo/karma.conf.js",
        "polyfills": ["zone.js", "zone.js/testing"]
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/ng-superclass/demo/**/*.ts",
          "packages/ng-superclass/demo/**/*.html"
        ]
      }
    }
  },
  "tags": ["lib:demo"]
}
