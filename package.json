    "typescript": "4.9.5",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
